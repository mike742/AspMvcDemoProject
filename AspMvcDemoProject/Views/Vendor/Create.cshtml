@model Vendor

<h2>Create new Vendor</h2>

<form asp-action="Create">
    <div class="form-group">
        <label asp-for="V_code"></label>
        <input type="text" asp-for="V_code" />
    </div>
    <div class="form-group">
        <label asp-for="V_name"></label>
        <input type="text" asp-for="V_name" />
    </div>
    <div class="form-group">
        <label asp-for="V_contact"></label>
        <input type="text" asp-for="V_contact" />
    </div>
    <div class="form-group">
        <label asp-for="V_AreaCode"></label>
        <input type="text" asp-for="V_AreaCode" />
    </div>
    <div class="form-group">
        <label asp-for="V_phone"></label>
        <input type="text" asp-for="V_phone" />
    </div>
    <div class="form-group">
        <label asp-for="V_state"></label>
        <input type="text" asp-for="V_state" />
    </div>
    <div class="form-group">
        <label asp-for="V_order"></label>
        <input type="text" asp-for="V_order" />
    </div>

    <button type="submit" class="btn btn-primary">Create</button>
</form>

<p>
